{
  "name": "@orinc/bluebird-ui",
  "version": "1.2.0",
  "description": "UI components built on Vue, mainly for admin panels.",
  "main": "dist/index.js",
  "scripts": {
    "serve": "webpack serve --config build/webpack.config.demo.js",
    "build": "npm run build:modern && npm run build:withie",
    "build:modern": "BROWSERSLIST_ENV=modern webpack --config build/webpack.config.js --mode=production",
    "build:withie": "BROWSERSLIST_ENV=withie webpack --config build/webpack.config.js --mode=production",
    "build:analyze": "webpack --config build/webpack.config.js --mode=production --analyze",
    "lint": "eslint ./ --ext=.js,.vue --fix",
    "release": "git push && rimraf dist && mkdir -p dist && npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orinc/bluebird-ui.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [],
  "author": "Meathill <meathill@openresty.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/orinc/bluebird-ui/issues"
  },
  "homepage": "https://github.com/orinc/bluebird-ui#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "bootstrap": "^4.5.3",
    "core-js": "^3.6.5",
    "css-loader": "^5.0.0",
    "eslint": "^7.11.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-vue": "^7.1.0",
    "html-webpack-plugin": "^4.5.0",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^1.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "pug": "^3.0.2",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "style-loader": "^2.0.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^4.1.1",
    "terser-webpack-plugin": "^5.0.0",
    "vue": "^3.0.1",
    "vue-loader": "^16.0.0-beta.8",
    "vue-router": "^4.0.0-beta.13",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.1.3",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.1.0"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.0.1",
    "eventemitter3": "^4.0.7",
    "moment": "^2.29.1",
    "webpack-dev-server": "^3.11.0"
  },
  "sideEffects": [
    "*.styl",
    "*.css"
  ]
}
